{
  "_version": "1.59.0",
  "sap.app": {
    "id": "sap.ee.demo.employee1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.13.1",
      "toolsId": "68d5dc83-c1c5-4d37-8a09-da9a4762b3a4"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/EmplyeeLeaveTrackerService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.122.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ee.demo.employee1.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "EmployeeList",
          "target": "EmployeeList"
        },
        {
          "pattern": "Employee({key}):?query:",
          "name": "EmployeeObjectPage",
          "target": "EmployeeObjectPage"
        },
        {
          "pattern": "Employee({key})/leaves({key2}):?query:",
          "name": "Employee_leavesObjectPage",
          "target": "Employee_leavesObjectPage"
        }
      ],
      "targets": {
        "EmployeeList": {
          "type": "Component",
          "id": "EmployeeList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Employee",
              "variantManagement": "Page",
              "navigation": {
                "Employee": {
                  "detail": {
                    "route": "EmployeeObjectPage"
                  }
                }
              }
            }
          }
        },
        "EmployeeObjectPage": {
          "type": "Component",
          "id": "EmployeeObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Employee",
              "controlConfiguration": {
                "leaves/@com.sap.vocabularies.UI.v1.LineItem#Leaves1": {
                  "tableSettings": {
                    "creationMode": {
                      "name": "NewPage"
                    }
                  }
                }
              },
              "navigation": {
                "leaves": {
                  "detail": {
                    "route": "Employee_leavesObjectPage"
                  }
                }
              }
            }
          }
        },
        "Employee_leavesObjectPage": {
          "type": "Component",
          "id": "Employee_leavesObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "contextPath": "/Employee/leaves",
              "navigation": {},
              "editableHeaderContent": false,
              "content": {
                "header": {
                  "visible": false
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
